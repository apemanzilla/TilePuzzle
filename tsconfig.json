{
  "compilerOptions": {
	"module": "commonjs",
	"target": "es6",
	"sourceMap": true,
	"noImplicitAny": true,
	"noImplicitReturns": true,
	"noImplicitThis": true,
	"allowUnreachableCode": false,
	"allowUnusedLabels": false,
	"noUnusedLocals": true,
	"noUnusedParameters": true,
	"allowSyntheticDefaultImports": true,
	"baseUrl": "ts"
  },
  "exclude": [
	"node_modules"
  ]
}